#!/usr/bin/env sh
PROJECT=`docker compose ls -q`
docker run --rm -a STDIN -a STDOUT -a STDERR --network ${PROJECT}_{{ volgactf.final.network.name }} --env-file ./env/postgres-common.env --env-file ./env/postgres-volgactf-admin.env --env VOLGACTF_FINAL_TEAM_LOGO_DIR=/opt/volgactf/final/team_logo -v ./team_logo:/opt/volgactf/final/team_logo volgactf/volgactf-final-backend:0.1.0 cli db:reset
RETVAL=$?
exit $RETVAL
